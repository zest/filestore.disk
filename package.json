{
    "name": "filestore.disk",
    "description": "filestore.disk is a filestore component for soul that manages files on disks",
    "version": "0.0.1",
    "main": "./lib/index",
    "private": true,
    "scripts": {
        "coverage": "grunt coverage",
        "cleanup": "grunt cleanup",
        "init": "grunt init",
        "test": "grunt test",
        "document": "grunt document",
        "observe": "grunt observe"
    },
    "maintainers": [
        {
            "name": "Pranav Jha",
            "email": "jha.pranav.s@gmail.com",
            "web": "https://github.com/soul-infra"
        }
    ],
    "licenses": [
        {
            "type": "unlicense",
            "url": "http://unlicense.org"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/soul-infra/filestore.disk.git"
        }
    ],
    "dependencies": {
        "q": "1.0.1",
        "fs-extra": "*",
        "merge": "*",
        "findit": "*",
        "minimatch": "*",
        "tar.gz": "*"
    },
    "devDependencies": {
        "grunt": "0.4.5",
        "grunt-contrib-clean": "0.6.0",
        "grunt-contrib-watch": "0.6.1",
        "grunt-contrib-jshint": "0.10.0",
        "ink-docstrap": "0.4.12",
        "grunt-jsdoc": "0.5.7",
        "grunt-mocha-cov": "0.3.0",
        "mocha-term-cov-reporter": "0.2.0",
        "chai": "1.9.1",
        "sinon": "1.10.3",
        "sinon-chai": "2.5.0",
        "chai-as-promised": "4.1.1",
        "merge": "1.2.0",
        "base.logger": "soul-infra/base.logger"
