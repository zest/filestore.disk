    "directories": {
        "lib": "lib",
        "doc": "doc",
        "test": "test",
        "out": ".out"
    },
    "config": {
        "blanket": {
            "pattern": "//^(?!.*/node_modules/).*/lib/"
        }
    },
    "soul-component" : "filestore"
